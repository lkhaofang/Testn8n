      "name": "HTTP Request"
    },
    {
      "parameters": {
        "jsCode": "const HEADERS = {\n  'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36',\n  'Accept': 'application/json, text/plain, */*',\n  'Accept-Language': 'en-US,en;q=0.9,th;q=0.8',\n  'Connection': 'keep-alive',\n  'Referer': 'https://www.settrade.com/th/home',\n  'sec-ch-ua': '\" Not A;Brand\";v=\"99\", \"Chromium\";v=\"91\", \"Google Chrome\";v=\"91\"',\n  'sec-ch-ua-mobile': '?0',\n  'Sec-Fetch-Dest': 'empty',\n  'Sec-Fetch-Mode': 'cors',\n  'Sec-Fetch-Site': 'same-origin'\n};\n\nconst THRESHOLD = 10;\n\nasync function fetchStocks(url) {\n  const response = await fetch(url, { headers: HEADERS });\n  if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);\n  return response.json();\n}\n\nasync function main() {\n  const endpoints = {\n    gainers: 'https://www.settrade.com/api/set/market-summary/realtime/gainers',\n    losers: 'https://www.settrade.com/api/set/market-summary/realtime/losers',\n  };\n\n  let alerts = [];\n\n  for (const [category, url] of Object.entries(endpoints)) {\n    try {\n      const stocks = await fetchStocks(url);\n\n      if (!Array.isArray(stocks)) {\n        console.warn(`ข้อมูลจาก ${url} ไม่ใช่ array`);\n        continue;\n      }\n\n      for (const stock of stocks) {\n        const symbol = stock.symbol;\n        const changePercent = stock.changePercent;\n\n        if (symbol && changePercent !== undefined && Math.abs(changePercent) >= THRESHOLD) {\n          const direction = changePercent > 0 ? 'เพิ่มขึ้น' : 'ลดลง';\n          alerts.push({\n            json: {\n              symbol,\n              direction,\n              changePercent: Math.abs(changePercent).toFixed(2),\n              message: `หุ้น ${symbol} ${direction} ${Math.abs(changePercent).toFixed(2)}%`\n            }\n          });\n        }\n      }\n    } catch (err) {\n      alerts.push({\n        json: {\n          error: `เกิดข้อผิดพลาดในการดึงข้อมูลจาก ${url}: ${err.message}`\n        }\n      });\n    }\n  }\n\n  if (alerts.length === 0) {\n    alerts.push({\n      json: { message: 'ไม่มีหุ้นที่มีการเปลี่ยนแปลงราคามากกว่า 10% ในช่วงนี้' }\n    });\n  }\n\n  return alerts;\n}\n\nreturn main();\n"
      },
